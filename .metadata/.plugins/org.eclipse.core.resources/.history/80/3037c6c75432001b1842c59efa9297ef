import java.util.Scanner;

public class lb14 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int n,i;
		System.out.println("Enter two no");
		Scanner sc=new Scanner(System.in);
		n=sc.nextInt();
		for(i=1;i<=10;i++) {
			System.out.println(n+"*"+i+"="+n*i);	
		}	
	}

}
