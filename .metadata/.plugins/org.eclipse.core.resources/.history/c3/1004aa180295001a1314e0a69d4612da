import java.util.*;
public class j2
{
    public static void main(String  args[])
    {
        int x=8,y=-1,z;
        z=x*y;
        System.out.println(z);
        if(z<0)
        {
       System.out.println("negative");
        }
        else
        {
       System.out.println("Positive");
        }
            
       
       
    }
}