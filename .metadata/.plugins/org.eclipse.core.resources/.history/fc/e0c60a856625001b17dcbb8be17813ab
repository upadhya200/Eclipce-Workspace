import java.util.Scanner;
class jb{
	Scanner sc=new Scanner(System.in);
	int n;
	void max(int m) {
		m=n;
		System.out.println("Enter any no:");
		m=sc.nextInt();		
        }
	void min(int m) {
		m=n;
		if(m%2==0) {
			System.out.println("Even");
		}
	}
}
public class lb1 {
public static void main(String args[]) {
	jb obj=new jb();
	obj.max(0);
	obj.min(0);
}
}
